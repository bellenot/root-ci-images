[alias]
  st = status
  ci = commit
  co = checkout
  lola = log --graph --decorate=full\n--pretty=oneline --abbrev-commit --all
  logf = "!echo \"Remember to add -S<string>\" ; git log --color -p --source --all"
[color]
  ui = true
  diff = auto
  status = auto
[core]
  excludesfile = /root/.gitignore_global
  editor = vim
[branch]
  autosetuprebase = always
